{
  "name": "my-portfolio-steeltroops",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:no-lint": "vite --host",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "analyze": "bun run build:analyze",
    "bundle-size": "bun run build && bun scripts/bundle-size-check.js",
    "lint": "bunx eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "bunx --bun vitest",
    "test:ui": "bunx --bun vitest --ui",
    "test:run": "bunx --bun vitest run",
    "test:coverage": "bunx --bun vitest run --coverage",
    "add": "bun add",
    "remove": "bun remove",
    "update": "bun update",
    "npm": "echo 'Redirecting to Bun...' && bun",
    "yarn": "echo 'Redirecting to Bun...' && bun",
    "pnpm": "echo 'Redirecting to Bun...' && bun",
    "npm-install": "echo 'Using Bun instead of npm install'",
    "yarn-install": "echo 'Using Bun instead of yarn install'",
    "pnpm-install": "echo 'Using Bun instead of pnpm install'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.85.0",
    "@vercel/speed-insights": "^1.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^10.18.0",
    "highlight.js": "^11.10.0",
    "prismjs": "^1.30.0",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.2.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.18",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.17",
    "vite": "4.4.9",
    "vitest": "^3.2.4"
  }
}
